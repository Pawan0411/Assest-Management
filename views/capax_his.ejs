<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Assets Management</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
        integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <style>
        ul {
            list-style-type: none;
            margin: 0;
            padding: 0;
            overflow: hidden;
            background-color: white;
        }

        li {
            float: left;
        }

        li a {
            display: block;
            color: black;
            text-align: center;
            padding: 14px 16px;
            text-decoration: none;
        }

        li a:hover {
            background-color: #ffffff;
        }

        .sub-entry {
            width: 50%;
            float: left;
        }
    </style>
</head>

<body>
    <ul>
        <li><a href="./import_cap">Import</a></li>
        <li><a href="./search_c">Search</a></li>
        <li><a href="./revenue">Revenue</a></li>

    </ul>
    <%- locals.flash %>
    <div class="row">
        <div class="container">
            <h3 class="col-sm-6">Assets Management</h3>
            <legend class="col-sm-6">Capax</legend>
            <form action="/next" method="GET">
                <div class="sub-entry col-sm-6">
                    <div class="form-group">
                        <label for="exampleserailNumber">Serial Number</label>
                        <input type="text" readonly="readonly" class="form-control snumber" name="exampleserailNumber"
                            id="exampleserailNumber" value="<%=exampleserailNumber %>" placeholder="Enter Serial Number"
                            autocomplete="off">
                    </div>
                    <div class="form-group">
                        <label for="exampleassettype">Asset Type</label>
                        <select class="form-control asset" disabled="disabled" name="exampleassettype"
                            id="exampleassettype" autocomplete="off">
                            <option value="" disabled selected><%=exampleassettype %></option>
                            <option value="Desktop">Desktop</option>
                            <option value="Laptop">Laptop</option>
                            <option value="Printer">Printer</option>
                            <option value="Tablet">Tablet</option>
                            <option value="Server">Server</option>
                            <option value="WorkStation">WorkStation</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="exampleSapCode">Sap Code</label>
                        <input type="text" readonly="readonly" class="form-control sapcode" id="exampleSapCode"
                            name="exampleSapCode" value="<%=exampleSapCode %>" autocomplete="off"
                            placeholder="Enter Sap Code">
                    </div>
                    <div class="form-group">
                        <label for="exampleMaterialCode">Material Code</label>
                        <input type="text" readonly="readonly" class="form-control materialcode"
                            value="<%= exampleMaterialCode %>" name="exampleMaterialCode" id="exampleMaterialCode"
                            placeholder="Enter Material Code" autocomplete="off">
                    </div>
                    <div class="form-group">
                        <label for="exampleMaterialQuantity">Material Quantity</label>
                        <input type="number" readonly="readonly" class="form-control materialquantity"
                            name="exampleMaterialQuantity" id="exampleMaterialQuantity"
                            value="<%= exampleMaterialQuantity %>" placeholder="Enter Material Quantity"
                            autocomplete="off">
                    </div>
                    <div class="form-group">
                        <label for="examplepoNumber">PO Number</label>
                        <input type="text" readonly="readonly" class="form-control ponumber"
                            value="<%= examplepoNumber %>" id="examplepoNumber" name="examplepoNumber"
                            placeholder="Enter PO Number" autocomplete="off">
                    </div>
                    <div class="form-group">
                        <label for="examplepoDate">PO Date</label>
                        <input type="date" readonly="readonly" class="form-control podate" name="examplepoDate"
                            value="<%= examplepoDate %>" id="examplepoDate" placeholder="Enter PO Date"
                            autocomplete="off">
                    </div>
                    <div class="form-group">
                        <label for="exampleInvoiceDate">Invoice Date</label>
                        <input type="date" readonly="readonly" class="form-control invoicedate"
                            value="<%= exampleInvoiceDate %>" name="exampleInvoiceDate" id="exampleInvoiceDate"
                            autocomplete="off">
                    </div>
                    <div class="form-group">
                        <label for="exampleInvoiceNumber">Invoice Number</label>
                        <input type="text" readonly="readonly" class="form-control invoicenumber"
                            value="<%= exampleInvoiceNumber %>" name="exampleInvoiceNumber" id="exampleInvoiceNumber"
                            autocomplete="off">
                    </div>
                </div>
                <div class="sub-entry col-sm-6">

                    <div class="form-group">
                        <label for="exampleRecieveDate">Receive Date</label>
                        <input type="date" readonly="readonly" class="form-control receivedate"
                            value="<%= exampleRecieveDate %>" name="exampleRecieveDate" id="exampleRecieveDate"
                            autocomplete="off">
                    </div>
                    <div class="form-group">
                        <label for="exampleproccessor">Proccessor</label>
                        <input type="text" readonly="readonly" class="form-control proccessor" name="exampleproccessor"
                            id="exampleproccessor" value="<%= exampleproccessor %>" autocomplete="off"
                            placeholder="Enter Proccessor">
                    </div>
                    <div class="form-group">
                        <label for="examplehardisk">Hardisk</label>
                        <select class="form-control hardisk" disabled="disabled" name="examplehardisk"
                            id="examplehardisk" autocomplete="off">
                            <option value="" disabled selected><%= examplehardisk %></option>
                            <option value="500 GB">500 GB</option>
                            <option value="1 TB">1 TB</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="exampleram">Ram</label>
                        <select class="form-control ram" disabled="disabled" name="exampleram" id="exampleram"
                            autocomplete="off">
                            <option value="" disabled selected><%= exampleram %></option>
                            <option value="4 GB">4 GB</option>
                            <option value="8 GB">8 GB</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="exampleModel">Model</label>
                        <input type="text" readonly="readonly" class="form-control model" name="exampleModel"
                            value="<%= exampleModel %>" id="exampleModel" autocomplete="off" placeholder="Enter Model">
                    </div>
                    <div class="form-group">
                        <label for="exampleModelDescp">Model Description</label>
                        <textarea readonly="readonly" rows="6" style="height:211px;" type="textarea"
                            class="form-control modeldescp" id="exampleModelDescp" name="exampleModelDescp"
                            autocomplete="off" placeholder="Enter Model Description"><%= exampleModelDescp %></textarea>
                    </div>
                    <div class="form-group">
                        <label for="exampleUpdateSummit">Updated In Summit</label>
                        <select class="form-control summit" name="exampleUpdateSummit" id="exampleUpdateSummit"
                            disabled="disabled" autocomplete="off" placeholder="Enter Sap Code">
                            <option value="" disabled selected><%= exampleUpdateSummit%></option>
                            <option value="YES">YES</option>
                            <option value="NO">NO</option>
                        </select>
                    </div>
                </div>
                <button type="next" class="btn btn-success" id="next">Next</button>
                <button type="prev" class="btn btn-success" formaction="/prev" id="prev">Prev</button>
                <button type="del" class="btn btn-danger" formaction="/capax_del" id="del">Delete</button>

            </form>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
        integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
        integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
        crossorigin="anonymous"></script>
    <script src="https://www.gstatic.com/firebasejs/6.2.0/firebase-app.js"></script>

    <script src="https://www.gstatic.com/firebasejs/3.1.0/firebase-database.js"></script>
    <!-- <script src="app_c.js"></script> -->
</body>

</html>